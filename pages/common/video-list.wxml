<template name="videoItem">
  <view class="video-desc">{{content}}</view>
  <video id="{{'video' + id}}" class="video" src="{{src}}" controls objectFit="fill"></video>
  <view class="video-opt">
    <view class="video-opt-star" data-liked="{{liked}}" catchtap="videoLike" data-video-id="{{id}}"></view>
    <!--<view catchtap="videoShare" data-video-id="{{id}}"><text>Share</text></view>-->
  </view>
</template>